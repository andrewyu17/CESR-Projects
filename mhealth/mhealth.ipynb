   "metadata": {},
   "outputs": [],
   "source": [
    "global input_dir \"C:\\Users\\ayu087\\OneDrive\\Dropbox\\UKB\\Input\"\n",
    "global output_dir \"C:\\Users\\ayu087\\OneDrive\\Dropbox\\UKB\\Output\"\n",
